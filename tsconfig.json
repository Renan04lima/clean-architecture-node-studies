{
    "compilerOptions": {
      "outDir": "dist",
      "module": "commonjs",
      "target": "es2019",
      "esModuleInterop": true,
      "sourceMap": true, // faz o mapper entre o dist e os arquivos em ts
      "rootDirs": [
        "src"
      ],
      "baseUrl": "src",
      "paths": {
        "@/*": [
          "*"
        ]
      }
    },
    "include": [
      "src"
    ]
  }